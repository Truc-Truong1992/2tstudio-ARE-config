{
    "app_version": "2.0-Smart-Logic",
    "selectors": {
        "switch_trigger": {
            "primary": "xpath://div[@aria-label='Chuyển' and @role='button']",
            "fallback_text": "Chuyển",
            "role": "button"
        },
        "switch_confirm": {
            "primary": "xpath://div[@role='dialog']//div[@aria-label='Chuyển' and @role='button']",
            "fallback_text": "Chuyển",
            "role": "button"
        },
        "btn_upload_trigger": {
            "primary": "xpath://div[@aria-label='Ảnh/video' and @role='button']",
            "fallback_text": "Ảnh/video"
        },
        "input_caption": {
            "primary": "xpath://div[@role='textbox' and @contenteditable='true']",
            "attribute": "data-lexical-editor"
        },
        "btn_next_step": {
            "primary": "xpath://div[@aria-label='Tiếp' and @role='button']",
            "fallback_text": "Tiếp"
        },
        "btn_final_post": {
            "primary": "xpath://div[@aria-label='Đăng' and @role='button']",
            "fallback_text": "Đăng",
            "alternative_text": "Chia sẻ"
        },
        "check_points": {
            "is_edit_screen": "Chỉnh sửa thước phim",
            "is_final_screen": "Cài đặt thước phim",
            "is_success": "Đã đăng"
        }
    }
}
